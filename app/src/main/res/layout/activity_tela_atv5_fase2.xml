<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:name=".Tela_Atv5_fase2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">


    <ImageView
        android:id="@+id/btnVoltarAtv5Fase2"
        android:layout_width="40dp"
        android:layout_height="33dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar11"
        app:srcCompat="@drawable/seta_voltar" />

    <TextView
        android:id="@+id/txtEnunciadoSeq1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginEnd="69dp"
        android:background="#FFFFFF"
        android:fontFamily="@font/fonttitulopreto"
        android:text="@string/enun_fase2_atv5"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnVoltarAtv5Fase2" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar11"
        android:layout_width="411dp"
        android:layout_height="?attr/actionBarSize"
        android:background="#FFFFFF"
        android:elevation="4dp"
        android:padding="8dp"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:logo="@drawable/logo_colorida"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />

    <ImageView
        android:id="@+id/balaoSeq2"
        android:layout_width="239dp"
        android:layout_height="184dp"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEnunciadoSeq1"
        app:srcCompat="@drawable/balao_enun_letras_faltando" />

    <ImageView
        android:id="@+id/imageBoneco_fase2atv5"
        android:layout_width="130dp"
        android:layout_height="174dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEnunciadoSeq1"
        app:srcCompat="@drawable/avatar_enun" />

    <ImageView
        android:id="@+id/btn_NA"
        android:layout_width="200dp"
        android:layout_height="62dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_JP"
        app:layout_constraintTop_toBottomOf="@+id/sequencia1"
        app:srcCompat="@drawable/btn_letra_na" />

    <ImageView
        android:id="@+id/btn_JP"
        android:layout_width="200dp"
        android:layout_height="62dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_NA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sequencia1"
        app:srcCompat="@drawable/btn_letra_jp" />

    <ImageView
        android:id="@+id/btn_JPcerto"
        android:layout_width="200dp"
        android:layout_height="62dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_JP"
        app:layout_constraintEnd_toEndOf="@+id/btn_JP"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/btn_JP"
        app:layout_constraintTop_toTopOf="@+id/btn_JP"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/btn_letra_jp_certo" />

    <ImageView
        android:id="@+id/btn_NAerrada"
        android:layout_width="200dp"
        android:layout_height="62dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_NA"
        app:layout_constraintEnd_toEndOf="@+id/btn_NA"
        app:layout_constraintStart_toStartOf="@+id/btn_NA"
        app:layout_constraintTop_toTopOf="@+id/btn_NA"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/btn_na_errada" />

    <ImageView
        android:id="@+id/sequencia1"
        android:layout_width="382dp"
        android:layout_height="45dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/balaoSeq2"
        app:srcCompat="@drawable/seq_letras_atv5_fase2" />

    <ImageView
        android:id="@+id/j_verde"
        android:layout_width="35dp"
        android:layout_height="39dp"
        app:layout_constraintBottom_toBottomOf="@+id/sequencia1"
        app:layout_constraintEnd_toEndOf="@+id/sequencia1"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toStartOf="@+id/sequencia1"
        app:layout_constraintTop_toTopOf="@+id/sequencia1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/j_verde" />

    <ImageView
        android:id="@+id/j_vermelho"
        android:layout_width="34dp"
        android:layout_height="39dp"
        app:layout_constraintBottom_toBottomOf="@+id/sequencia1"
        app:layout_constraintEnd_toEndOf="@+id/sequencia1"
        app:layout_constraintHorizontal_bias="0.255"
        app:layout_constraintStart_toStartOf="@+id/sequencia1"
        app:layout_constraintTop_toTopOf="@+id/sequencia1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/j_vermelho" />

    <ImageView
        android:id="@+id/p_verde"
        android:layout_width="41dp"
        android:layout_height="39dp"
        app:layout_constraintBottom_toBottomOf="@+id/sequencia1"
        app:layout_constraintEnd_toEndOf="@+id/sequencia1"
        app:layout_constraintHorizontal_bias="0.876"
        app:layout_constraintStart_toStartOf="@+id/sequencia1"
        app:layout_constraintTop_toTopOf="@+id/sequencia1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/p_verde" />

    <ImageView
        android:id="@+id/p_vermelho"
        android:layout_width="30dp"
        android:layout_height="39dp"
        app:layout_constraintBottom_toBottomOf="@+id/sequencia1"
        app:layout_constraintEnd_toEndOf="@+id/sequencia1"
        app:layout_constraintHorizontal_bias="0.849"
        app:layout_constraintStart_toStartOf="@+id/sequencia1"
        app:layout_constraintTop_toTopOf="@+id/sequencia1"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/p_vermelho" />

    <ImageView
        android:id="@+id/imgTriang"
        android:layout_width="29dp"
        android:layout_height="29dp"
        app:layout_constraintBottom_toBottomOf="@+id/sequencia1"
        app:layout_constraintEnd_toEndOf="@+id/sequencia1"
        app:layout_constraintHorizontal_bias="0.252"
        app:layout_constraintStart_toStartOf="@+id/sequencia1"
        app:layout_constraintTop_toTopOf="@+id/sequencia1"
        app:layout_constraintVertical_bias="0.625"
        app:srcCompat="@drawable/triangulo" />

    <ImageView
        android:id="@+id/imgLosangulo"
        android:layout_width="29dp"
        android:layout_height="29dp"
        app:layout_constraintBottom_toBottomOf="@+id/sequencia1"
        app:layout_constraintEnd_toEndOf="@+id/sequencia1"
        app:layout_constraintHorizontal_bias="0.847"
        app:layout_constraintStart_toStartOf="@+id/sequencia1"
        app:layout_constraintTop_toTopOf="@+id/sequencia1"
        app:layout_constraintVertical_bias="0.625"
        app:srcCompat="@drawable/losangulo" />

</androidx.constraintlayout.widget.ConstraintLayout>
