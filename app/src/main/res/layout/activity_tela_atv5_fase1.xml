<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Tela_Atv5_fase1">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar6"
        android:layout_width="0dp"
        android:layout_height="?attr/actionBarSize"
        android:background="#FFFFFF"
        android:elevation="4dp"
        android:padding="8dp"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:logo="@drawable/logo_colorida"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />

    <ImageView
        android:id="@+id/btnVoltarAtv5Fase1"
        android:layout_width="40dp"
        android:layout_height="33dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.037"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar6"
        app:srcCompat="@drawable/seta_voltar" />

    <TextView
        android:id="@+id/txtEnunAtv5Fase1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="84dp"
        android:fontFamily="@font/fonttitulopreto"
        android:text="@string/enun_qual_letra"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.416"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar6" />

    <ImageView
        android:id="@+id/imageEnunciado_fase1"
        android:layout_width="130dp"
        android:layout_height="174dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.149"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtEnunAtv5Fase1"
        app:srcCompat="@drawable/avatar_enun" />

    <ImageView
        android:id="@+id/ImageBalaofase1"
        android:layout_width="223dp"
        android:layout_height="199dp"
        android:layout_marginTop="76dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.845"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar6"
        app:srcCompat="@drawable/bal_oletraupng_fase1" />

    <ImageView
        android:id="@+id/btn_Nm1"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_LetU"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ImageBalaofase1"
        app:srcCompat="@drawable/n1_fase1" />

    <ImageView
        android:id="@+id/btn_LetU"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_Nm1"
        app:layout_constraintTop_toBottomOf="@+id/ImageBalaofase1"
        app:srcCompat="@drawable/letrau_fase1" />

    <ImageView
        android:id="@+id/btn_1_inc"
        android:layout_width="128dp"
        android:layout_height="116dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_Nm1"
        app:layout_constraintEnd_toEndOf="@+id/btn_Nm1"
        app:layout_constraintStart_toStartOf="@+id/btn_Nm1"
        app:layout_constraintTop_toTopOf="@+id/btn_Nm1"
        app:srcCompat="@drawable/n1inc_fase1" />

    <ImageView
        android:id="@+id/btn_U_Correta"
        android:layout_width="134dp"
        android:layout_height="121dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_LetU"
        app:layout_constraintEnd_toEndOf="@+id/btn_LetU"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="@+id/btn_LetU"
        app:layout_constraintTop_toTopOf="@+id/btn_LetU"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/corretaletrau_fase1" />

    <ImageView
        android:id="@+id/btn_LetS"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_LetD"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Nm1"
        app:srcCompat="@drawable/letra_s_fase1" />

    <ImageView
        android:id="@+id/btn_S_inc"
        android:layout_width="128dp"
        android:layout_height="116dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_LetS"
        app:layout_constraintEnd_toEndOf="@+id/btn_LetS"
        app:layout_constraintStart_toStartOf="@+id/btn_LetS"
        app:layout_constraintTop_toTopOf="@+id/btn_LetS"
        app:srcCompat="@drawable/incorretaletras_fase1" />

    <ImageView
        android:id="@+id/btn_LetD"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_LetS"
        app:layout_constraintTop_toBottomOf="@+id/btn_U_Correta"
        app:srcCompat="@drawable/letrad_fase1" />

    <ImageView
        android:id="@+id/btn_D_inc"
        android:layout_width="128dp"
        android:layout_height="116dp"
        app:layout_constraintBottom_toBottomOf="@+id/btn_LetD"
        app:layout_constraintEnd_toEndOf="@+id/btn_LetD"
        app:layout_constraintStart_toStartOf="@+id/btn_LetD"
        app:layout_constraintTop_toTopOf="@+id/btn_LetD"
        app:srcCompat="@drawable/incorretaletrad_fase1" />

</androidx.constraintlayout.widget.ConstraintLayout>
