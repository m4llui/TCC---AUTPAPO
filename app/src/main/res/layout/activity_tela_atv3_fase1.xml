<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".Tela_Atv3_fase1">

    <ImageView
        android:id="@+id/btnVoltarAtv3Fase1"
        android:layout_width="40dp"
        android:layout_height="33dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.07"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3"
        app:srcCompat="@drawable/seta_voltar" />

    <ImageView
        android:id="@+id/imageEnunciado3"
        android:layout_width="343dp"
        android:layout_height="253dp"


        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.382"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3"
        app:srcCompat="@drawable/boneco_enunciado" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar3"
        android:layout_width="0dp"
        android:layout_height="?attr/actionBarSize"
        android:background="#FFFFFF"
        android:elevation="4dp"
        android:padding="8dp"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:logo="@drawable/logo_colorida"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light" />

    <ImageView
        android:id="@+id/btn_K"
        android:layout_width="128dp"
        android:layout_height="116dp"

        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_I"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageEnunciado3"
        app:srcCompat="@drawable/letrak" />

    <ImageView
        android:id="@+id/btn_I"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="56dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageEnunciado3"
        app:srcCompat="@drawable/letrai" />

    <ImageView
        android:id="@+id/btn_O"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_K"
        app:srcCompat="@drawable/letraopng" />

    <ImageView
        android:id="@+id/btn_G"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toEndOf="@+id/btn_O"
        app:layout_constraintTop_toBottomOf="@+id/btn_I"
        app:srcCompat="@drawable/letrag" />

    <ImageView
        android:id="@+id/imgBalao3"
        android:layout_width="178dp"
        android:layout_height="204dp"
        android:layout_marginTop="68dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.819"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar3"
        app:srcCompat="@drawable/bal_ocomletrai" />

    <ImageView
        android:id="@+id/btn_K_errado"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_I"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageEnunciado3"
        app:srcCompat="@drawable/incorretaletrak" />

    <ImageView
        android:id="@+id/btn_O_errado"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_G"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_K"
        app:srcCompat="@drawable/incorretaletrao" />

    <ImageView
        android:id="@+id/btn_G_errado"
        android:layout_width="128dp"
        android:layout_height="116dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toEndOf="@+id/btn_O"
        app:layout_constraintTop_toBottomOf="@+id/btn_I"
        app:srcCompat="@drawable/incorretaletrag" />

    <ImageView
        android:id="@+id/btn_I_Certo"
        android:layout_width="131dp"
        android:layout_height="121dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_K"
        app:layout_constraintTop_toBottomOf="@+id/imageEnunciado3"
        app:srcCompat="@drawable/corretaletrai" />

</androidx.constraintlayout.widget.ConstraintLayout>